if [[ $- =~ i ]]; then
  if [ -n "$BASH_VERSION" ]; then
    export PATH=$(dirname ${BASH_SOURCE[0]})/bin:$PATH
    source $(dirname ${BASH_SOURCE[0]})/lib/bash_completion
  elif [ -n "$ZSH_VERSION" ]; then
    export PATH=$(dirname "${(%):-%x}")/bin:$PATH
    fpath=($(dirname "${(%):-%x}")/lib $fpath)
  else
    echo "Not suppported"
  fi
fi

